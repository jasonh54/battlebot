    baselayer.update(collidelayer);
    coverlayer.update(collidelayer);
    collidelayer.update();
    toplayer.update(collidelayer);
  }
  
  void totalDraw() {
    baselayer.draw();
    coverlayer.draw();
    collidelayer.draw();
    toplayer.draw();
  }
}
